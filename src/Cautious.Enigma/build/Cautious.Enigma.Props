<Project TreatAsLocalProperty="TaskAssembly">
  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <!-- Name of this file must match package ID. -->

  <PropertyGroup>
    <TaskAssembly>$(MSBuildThisFileDirectory)..\tasks\netstandard2.0\Cautious.Enigma.dll</TaskAssembly>
    <OrasExe>$(MSBuildThisFileDirectory)..\tools\win-x64\oras.exe</OrasExe>
  </PropertyGroup>

  <UsingTask TaskName="MSBuildTasks.OrasPush" AssemblyFile="$(TaskAssembly)" />

</Project>
